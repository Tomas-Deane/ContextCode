{
    "web": {
        "host": "localhost",
        "port": 54545,
        "debug": true
    },
    "logging_config": {
        "console_output": {
            "enabled": true,
            "level": "INFO",
            "format": "%(levelname).1s:[%(name)s]> %(message)s",
            "date_format": "%Y-%m-%d %H:%M:%S"
        },
        "file_output": {
            "enabled": true,
            "level": "DEBUG",
            "format": "%(asctime)s -%(levelname)s - %(name)s -  %(pathname)s:%(lineno)d - %(message)s",
            "date_format": "%Y-%m-%d %H:%M:%S",
            "log_dir": "logs",
            "filename": "app.log",
            "max_bytes": 10485760,
            "backup_count": 5
        }
    }
}